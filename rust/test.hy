for i in 0 upto 1 + 2 * 3 / 2 - 1 times 2 do
    var padding = 10;

    for i in 0 through i do
        print_int(i + 1 - 1 + padding);
    end

    i = 100;
    print_int(i);
end

var i, j = 11, 12;
print_int(i + j);

i, j = 21, 22;
print_int(i + j);

var done = 0;
while done <= 5 do
    print_int(1000000);
    done = done + 1;
end